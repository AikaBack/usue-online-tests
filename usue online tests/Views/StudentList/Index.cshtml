@model List<IGrouping<string, User>>

@{
    ViewData["Title"] = "Списки студентов";
}
<div class="prose">
    <h1>Списки студентов</h1>

    @for (int i = 0; i < Model.Count; i++)
    {
        <div>
            <h3>@Model[i].Key:</h3>
            <ul>
            @foreach (User user in Model[i])
            {
                <li>@user.Name</li>
            }
            </ul>
        </div>
    }
</div>